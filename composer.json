{
  "name": "mrfrkayvaz/privata",
  "description": "A Laravel package that provides tools for masking, anonymizing, and managing personal data to help with privacy compliance.",
  "keywords": ["laravel", "privacy", "personal-data"],
  "license": "MIT",
  "authors": [
    {
      "name": "Ã–mer Faruk Ayvaz",
      "email": "mrfrkayvaz@gmail.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "ext-openssl": "*",
    "illuminate/support": "^12.0",
    "illuminate/database": "^12.0"
  },
  "autoload": {
    "psr-4": {
      "Privata\\": "src/",
      "Privata\\Tests\\": "tests"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Privata\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Privata\\PrivataServiceProvider"
      ],
      "aliases": {
        "Privata": "Privata\\Facades\\Privata"
      }
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require-dev": {
    "orchestra/testbench": "^10.6",
    "pestphp/pest": "^4.0",
    "pestphp/pest-plugin-laravel": "^4.0",
    "phpstan/phpstan": "^1.10.50"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "scripts": {
    "test": "vendor/bin/pest",
    "analyse": "phpstan analyse --memory-limit=1G --no-progress",
    "baseline": "./vendor/bin/phpstan analyse --memory-limit=2G --generate-baseline"
  }
}
